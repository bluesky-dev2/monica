{
    "name": "djaiss/monica",
    "description": "Monica is a personal CRM.",
    "license": "AGPL",
    "keywords": ["prm", "crm", "social"],
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "ext-intl": "*",
        "bacon/bacon-qr-code": "^1.0",
        "creativeorange/gravatar": "~1.0",
        "doctrine/dbal": "^2.5",
        "erusev/parsedown": "~1.6",
        "fzaninotto/faker": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "jenssegers/date": "^3.3",
        "jeroendesloovere/vcard": "^1.5",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^4.0",
        "laravel/socialite": "^3.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.23",
        "paragonie/constant_time_encoding": "^2.2",
        "pragmarx/google2fa": "^2.0",
        "pragmarx/google2fa-laravel": "dev-master",
        "pragmarx/recovery": "^0.1.0",
        "predis/predis": "^1.1",
        "roave/security-advisories": "dev-master",
        "sabre/vobject": "^4.1",
        "sentry/sentry-laravel": "^0.7.0",
        "symfony/translation": "3.4.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3",
        "enm1989/chromedriver": "^2.36",
        "filp/whoops": "~2.0",
        "khanamiryan/qrcode-detector-decoder": "^1.0",
        "laravel/dusk": "^2.0",
        "matthiasnoback/live-code-coverage": "^0.1.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpcov": "^4.0",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "suggest": {
        "ext-apcu": "*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "apcu-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
