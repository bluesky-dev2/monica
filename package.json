    "font-awesome": "^4.7.0",
    "gulp": "^3.9.1",
    "hint.css": "^2.3.2",
    "jquery": "^3.3.1",
    "laravel-mix": "^4.0.12",
    "list.js": "^1.5.0",
    "lodash": "^4.17.5",
